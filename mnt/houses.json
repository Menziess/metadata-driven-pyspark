{
  "input": {
    "format": "csv",
    "path": "../mnt/houses/**/*.csv",
    "schema": {
      "type": "struct",
      "fields": [
        {
          "name": "id",
          "type": "long",
          "nullable": true,
          "metadata": {}
        },
        {
          "name": "city",
          "type": "string",
          "nullable": true,
          "metadata": {}
        }
      ]
    },
    "options": {
      "basePath": "../mnt/houses/",
      "header": true,
      "sep": ","
    }
  },
  "output": {
    "format": "parquet"
  },
  "mapping": [
    {
      "fieldname": "id"
    },
    {
      "fieldname": "city"
    }
  ]
}
